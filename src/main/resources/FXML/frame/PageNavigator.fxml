<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.HBox?>

<HBox fx:id="hboxPageNavigator" alignment="CENTER_RIGHT" minHeight="-Infinity" prefWidth="200.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ank.codeviewer.controller.frame.PageNavigatorController">
        <children>
            <Button mnemonicParsing="false" onAction="#onPreviousPageButtonClick" text="&lt;" />
            <ComboBox fx:id="cbPageNumbers" minHeight="-Infinity" prefWidth="150.0" />
            <Button mnemonicParsing="false" onAction="#onNextPageButtonClick" text="&gt;" />
        </children>
    </HBox>
